<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_type_purchase_prediction" model="ir.sequence.type">
            <field name="name">Purchase Prediction</field>
            <field name="code">purchase.prediction</field>
        </record>

        <record id="seq_purchase_prediction" model="ir.sequence">
            <field name="name">Purchase Prediction</field>
            <field name="code">purchase.prediction</field>
            <field name="prefix">AC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_type_purchase_prediction_line" model="ir.sequence.type">
            <field name="name">Purchase Prediction Line</field>
            <field name="code">purchase.prediction.line</field>
        </record>

        <record id="seq_purchase_prediction_line" model="ir.sequence">
            <field name="name">Purchase Prediction Line</field>
            <field name="code">purchase.prediction.line</field>
            <field name="prefix">PL</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
